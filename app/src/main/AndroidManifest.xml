<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.example.rpg_dice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_dd_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_dd_app_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HistoryActivity"></activity>
        <activity android:name=".DiceResultActivity" />
        <activity android:name=".DicesActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>